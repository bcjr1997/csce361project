<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".sosCall">

    <Button
        android:id="@+id/callHelper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="263dp"
        android:layout_marginEnd="180dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="439dp"
        android:text="Pussy"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/policeNumberText"
        android:layout_width="268dp"
        android:layout_height="51dp"
        android:layout_marginBottom="277dp"
        android:layout_marginEnd="96dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="59dp"
        android:text="Police: 911"
        app:layout_constraintBottom_toTopOf="@+id/callHelper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/ambulanceNumberText"
        android:layout_width="246dp"
        android:layout_height="46dp"
        android:layout_marginBottom="588dp"
        android:layout_marginEnd="304dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="57dp"
        android:text="Ambulance: 911"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/policeNumberText"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/helperPhoneText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="136dp"
        android:layout_marginEnd="100dp"
        android:layout_marginStart="190dp"
        android:layout_marginTop="122dp"
        android:text="HamsterBuddy's Number:"
        app:layout_constraintBottom_toTopOf="@+id/callHelper"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.901"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ambulanceNumberText"
        app:layout_constraintVertical_bias="1.0" />
</android.support.constraint.ConstraintLayout>